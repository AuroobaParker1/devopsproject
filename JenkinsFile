pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                docker-compose build
                docker-compose up
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}